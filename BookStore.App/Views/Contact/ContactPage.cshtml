@model List<ContactDto>

<link rel="stylesheet" href="~/css/upperStyles.css" asp-append-version="true">
<link rel="stylesheet" href="~/css/contactsPageStyles.css" asp-append-version="true">

<div class="PanelLeft">
    <div class="outClass">
        <div class="contactsDiv">
            <div class="header">
                <h2>Наши контакты</h2>
            </div>
        </div>
        @{
            foreach (var item in Model)
            {
                <div class="contactsDiv">
                    <div class="name">
                        <h2>@item.Name</h2>
                    </div>
                    <div class="body">
                        <h2 class="rightText">@item.Mail</h2>
                        <h2 class="rightText">@item.Number</h2>
                    </div>
                </div>
            }
        }
    </div>
</div>